{
  "name": "@sellgar/kit",
  "type": "module",
  "version": "0.0.125",
  "description": "Sellgar kit",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pyatakovvg/sellgar.ui.kit.git"
  },
  "scripts": {
    "build": "vite build",
    "login": "yarn npm login",
    "publish": "yarn npm publish"
  },
  "files": [
    "./dist/**/*",
    "./types/**/*"
  ],
  "exports": {
