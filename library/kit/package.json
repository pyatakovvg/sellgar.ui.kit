    "react-dom": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/react-slider": "1.3.6",
    "@vitejs/plugin-react": "5.0.4",
    "typescript": "5.9.3",
    "vite": "7.1.12",
    "vite-plugin-dts": "4.5.4",
    "vite-plugin-lib-inject-css": "2.2.2",
    "vite-plugin-webfont-dl": "3.11.1"
  },
  "bugs": {
