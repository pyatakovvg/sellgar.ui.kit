    "classnames": "2.5.1",
    "date-fns": "4.1.0",
    "moment": "2.30.1",
    "overlayscrollbars": "2.12.0",
    "overlayscrollbars-react": "0.5.6",
    "react-baron": "3.0.0",
    "react-slider": "2.0.6"
